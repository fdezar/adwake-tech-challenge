{
    "builds": [
      {
        "src": "dist/client/browser/**",
        "use": "@vercel/static"
      },
      {
        "src": "dist/client/server/**/*.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/favicon.ico",
        "dest": "/dist/client/browser/favicon.ico"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist/client/browser/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/client/server/main.server.mjs"
      }
    ]
  }
  
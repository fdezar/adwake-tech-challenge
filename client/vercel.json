{
    "builds": [
      {
        "src": "dist/app-name/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/favicon.ico",
        "dest": "/dist/app-name/favicon.ico"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist/app-name/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/app-name/index.html"
      }
    ]
  }